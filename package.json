{
  "name": "react-clickoutside",
  "version": "2.0.0",
  "description": "Higher order component for detecting when a click event has occured outside of a given component",
  "repository": {
    "url": "https://github.com/viralganatra/react-clickoutside.git",
    "type": "git"
  },
  "author": "Viral Ganatra <viral@viralganatra.com>",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "compile": "NODE_ENV=production rollup --config ./config/rollup.js",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:ci": "npm run lint && npm run test",
    "prepublish": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "hoc",
    "click",
    "outside",
    "function as child component",
    "clickoutside"
  ],
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "invariant": "^2.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.2.0",
    "jest-cli": "^23.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7"
  },
  "peerDependencies": {
    "react": "^15.0 || ^16.0"
  },
  "jest": {
    "setupFiles": [
      "./config/jest.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
